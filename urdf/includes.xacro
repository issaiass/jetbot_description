<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Rviz Color Properties -->
  <xacro:include filename="$(find jetbot_description)/urdf/includes/materials/materials.xacro" />  
  <!-- Rviz Color Properties -->  

  <!-- Gazebo Properties -->
  <xacro:include filename="$(find jetbot_description)/urdf/includes/gazebo/jetbot.gazebo.xacro" />  
  <!-- Gazebo Properties -->  

  <!-- Transmission Macro -->
  <xacro:include filename="$(find jetbot_description)/urdf/includes/gazebo/transmission.xacro" />    
  <!-- Transmission Macro -->

  <!-- Inertia Macros -->
  <xacro:include filename="$(find jetbot_description)/urdf/includes/gazebo/inertias.xacro" />  
  <!-- Inertia Macros -->

  <!-- Chassis macro -->
  <xacro:include filename="$(find jetbot_description)/urdf/includes/parts/chassis.xacro" />  
  <!-- Chassis macro -->

  <!-- Antenna Macro -->
  <xacro:include filename="$(find jetbot_description)/urdf/includes/parts/antenna.xacro" />  
  <!-- Antenna Macro -->

  <!-- Wheel Macro -->
  <xacro:include filename="$(find jetbot_description)/urdf/includes/parts/wheels/wheel.xacro" />  
  <!-- Wheel Macro -->

  <!-- Caster Wheel Macro -->
  <xacro:include filename="$(find jetbot_description)/urdf/includes/parts/wheels/caster.xacro" />  
  <!-- Caster Wheel Macro -->

  <!-- Camera-->
  <xacro:include filename="$(find jetbot_description)/urdf/includes/parts/sensors/rgbcam/camera.xacro" />
  <!-- Camera-->

  <!-- PCB Boards -->
  <xacro:include filename="$(find jetbot_description)/urdf/includes/parts/boards.xacro" />  
  <!-- PCB Boards -->

  <!-- Intel Realsense Macro -->
  <xacro:include filename="$(find jetbot_description)/urdf/includes/parts/sensors/realsense/_d435.urdf.xacro" />    
  <!-- Intel Realsense Macro -->

  <!-- RPLidar -->
  <xacro:include filename="$(find jetbot_description)/urdf/includes/parts/sensors/laser/laser.urdf.xacro" />    
  <!-- RPLidar -->
  
  <!-- IMU -->
  <xacro:include filename="$(find jetbot_description)/urdf/includes/parts/sensors/imu/bno055.urdf.xacro" />
  <!-- IMU -->

  <!-- GPS -->
  <xacro:include filename="$(find jetbot_description)/urdf/includes/parts/sensors/gps/mtk3339.urdf.xacro" />  
  <!-- GPS -->

  <!-- Sonar -->
  <xacro:include filename="$(find jetbot_description)/urdf/includes/parts/sensors/sonar/hcsr04.urdf.xacro" />  
  <!-- SOnar -->

</robot>